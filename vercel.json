{
  "rewrites": [
    {
      "source": "/.well-known/fc-miniapp.json",
      "destination": "/api/fc-miniapp"
    }
  ]
},
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "application/json; charset=utf-8"
        }
      ]
    }
  ]
}

